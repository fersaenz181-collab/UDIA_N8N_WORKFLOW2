    {
      "parameters": {
        "jsCode": "return items.map(item => {\n\n  let image = null;\n\n  // 1. Intentar extraer desde content:encoded\n  const encoded = item.json[\"content:encoded\"];\n\n  if (encoded) {\n    const match = encoded.match(/<img[^>]+src=\"([^\">]+)\"/i);\n    if (match && match[1]) {\n      image = match[1];\n    }\n  }\n\n  // 2. Fallback a enclosure.url\n  if (!image && item.json.enclosure && item.json.enclosure.url) {\n    image = item.json.enclosure.url;\n  }\n\n  // 3. Añadir propiedad image\n  item.json.image = image;\n\n  return item;\n});\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
